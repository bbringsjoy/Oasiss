    "type": "project",
    "require": {
        "doctrine/orm": "^3.5",
        "doctrine/dbal": "^4.3",
        "symfony/cache": "^7.3",
        "vlucas/phpdotenv": "^5.6"
    },
   "autoload": {
    "psr-4": {
        "App\\": "App/"
    }
},
